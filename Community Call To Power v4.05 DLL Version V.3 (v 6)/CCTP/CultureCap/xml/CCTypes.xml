<GameData>

	<!-- CCTypes (aka Themes) of different Cultural Capitals-->
	<Table name="CCTypes">
		<Column name="ID" type="integer" primarykey="true" autoincrement="true"/>
		<Column name="Type" type="text" unique="true"/>
		<Column name="Title" type="text" default="NULL" />
		<Column name="Description" type="text" reference="Language_en_US(Tag)" default="NULL"/>
		<Column name="IconAtlas" type="text" default="NULL" reference="IconTextureAtlases(Atlas)"/>
		<Column name="PortraitIndex" type="integer" default="-1"/>
		<Column name="SpecialistBuildingRequired" type="text" reference="Specialists(Type)" default="NULL"/>
		<Column name="BuildingClassRequired" type="text" reference="BuildingClasses(Type)" default="NULL" />
		<Column name="NoOfWondersRequired" type="integer" default="0"/>
		<Column name="NoOfCitiesRequired" type="integer" default="0" /> <!-- negative values mean that the player is only allowed to have at most the number of cities; also scales with mapsize-->
		<Column name="CitySizeRequired" type="integer" default="0"/> <!-- negative values mean that the citysize must at most the number -->
		<Column name="TechRequired" type="text" reference="Technologies(Type)" default="NULL" />
		<Column name="EraRequired" type="text" reference="Eras(Type)" default="NULL" />
		<Column name="PolicyRequired" type="text" reference="Policies(Type)" default="NULL" />
		<Column name="GarrisonRequired" type="boolean" default="false"/>
		<Column name="BuildingClassInEmpireRequired" type="text" reference="BuildingClasses(Type)" default="NULL" />
		<Column name="BuildingClassExcluded" type="text" reference="BuildingClasses(Type)" default="NULL" />
		<Column name="BuildingClassInEmpireExcluded" type="text" reference="BuildingClasses(Type)" default="NULL" />
		<Column name="MustProduce" type="boolean" default="false" /> <!-- defines if the city needs to produce something (units or buildings) -->
		<Column name="CanHaveResource" type="text" reference="Resources(Type)" default="NULL" />
		<Column name="ObsoleteTech" type="text" reference="Technologies(Type)" default="NULL" />
		<Column name="ObsoleteEra" type="text" reference="Eras(Type)" default="NULL" />
		<Column name="AIViabilityContext" type="text" reference="CCAIViabilityContexts(Type)" default="NULL" /> <!-- Defines a context that checks the viability of the theme for the AI -->
		<Column name="DisplayListing" type="float" default="0.0" /> <!-- Used for the sorting function in the popup - is a quasi reference to eras -->
		<Column name="IsEnabled" type="boolean" default="true" />
		<Column name="IsDebug" type="boolean" default="false" /> <!-- (for Debugging purposes) Deactivates Requirements so it will be always available if set to true-->
	</Table>
	<Table name="CCType_Flavors">
		<Column name="CCType" type="text" reference="CCTypes(Type)"/>
		<Column name="FlavorType" type="text" reference="Flavors(Type)"/>
		<Column name="Flavor" type="integer"/>
	</Table>
	
