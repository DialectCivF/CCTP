<GameData>
  <Table name="CCBonusTypes">
    <Column name="ID" type="integer" primarykey="true" autoincrement="true"/>
    <Column name="Type" type="text" unique="true"/>
    <Column name="BaseValue" type="float" default="0.0"/>
    <Column name="IsMalus" type="boolean" default="false" /> <!-- defines if it is a bonus or malus (ie. resulting value is multiplied by -1) -->
    <Column name="IncreasePerEra" type="float" default="0.0"/> <!-- The percental increase per era, ie: value = baseValue + baseValue * (era * increasePerEra), whereas era = [0(ancient), 7(future)]-->
    <Column name="IncreasePerElection" type="float" default="0.0" /> <!-- Analogue to IncreasePerEra-->
    <Column name="Context" type="text" reference="CCContextTypes(Type)" default="NULL"/> <!-- Used in the implementation to determine which city:methods to use-->
    <Column name="ContextRefType1" type="text" /> <!-- depending on the context, a reftype determines the type to change (i.e. YIELD_PRODUCTION, or SPECIALIST_MERCHANT), if needed -->
    <Column name="ContextRefType2" type="text" />
    <Column name="Description" type="text" reference="Language_en_US(Tag)" default="NULL"/>
    <Column name="IsTemporary" type="boolean" default="false"/> <!-- Defines if the bonus is permanent or temporary, meaning the bonuses will get removed when another election occurs-->
		<Column name="MapScaler" type="text" default="NULL" /> <!-- Value gets scaled based on this entry in CCMapScales Table -->
		<Column name="SpeedScaler" type="text" default="NULL" /> <!-- Value gets scaled based on this entry in CCSpeedScales Table -->
		<Column name="DisplayUseNegative" type="boolean" default="false"/> <!-- Determines if the negative of the value should be displayed -->
	</Table>
  
